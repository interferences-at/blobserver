<Blobserver>
    <Flow>
        <Actuator>
            <Type>Actuator_FiducialTracker</Type>
            <Param>
                <Name>treesFilename</Name>
                <Value>fiducials_default.trees</Value>
            </Param>
        </Actuator>
        <Source>
            <Type>Source_2D_OpenCV</Type>
            <Subsource>300</Subsource>
            <Param>
                <Name>framerate</Name>
                <Value>15</Value>
            </Param>
            <Param>
                <Name>iso</Name>
                <Value>400</Value>
            </Param>
            <Param>
                <Name>width</Name>
                <Value>640</Value>
            </Param>
            <Param>
                <Name>height</Name>
                <Value>480</Value>
            </Param>
            <Param>
                <Name>exposureTime</Name>
                <Value>500</Value>
            </Param>
            <Param>
                <Name>autoExposure</Name>
                <Value>300 200 80 40 127 32 0.05</Value>
            </Param>
            <Param>
                <Name>whiteBalanceRed</Name>
                <Value>512</Value>
            </Param>
            <Param>
                <Name>whiteBalanceBlue</Name>
                <Value>512</Value>
            </Param>
            <Param>
                <Name>save</Name>
                <Value>0 5 /home/manu/tmp/test_</Value>
            </Param>
        </Source>
        <Client>
            <Address>localhost</Address>
            <Port>9000</Port>
        </Client>
        <Server>
            <Address>localhost</Address>
            <Port>9002</Port>
        </Server>
    </Flow>
</Blobserver>
